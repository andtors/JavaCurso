package entities;

public class Persons {
	private double altura;
	private char genero;
	
	public double getAltura() {
		return altura;
	}
	public void setAltura(double altura) {
		this.altura = altura;
	}
	public String getGenero() {
		return genero;
	}
	public void setGenero(String genero) {
		this.genero = genero;
	}
	
	public Persons(double altura, String genero) {
	
		this.altura = altura;
		this.genero = genero;
	}
}
